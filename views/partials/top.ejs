  <!DOCTYPE html>
  <html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GooseHosting INC</title>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400&family=Roboto:wght@400&family=Open+Sans:wght@400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  </head>
  <body>
    <div class="top">
      <div class="navbar"> 
        <div class="left">
          <img src="https://cdn.discordapp.com/icons/1190316363105972314/b6b36f767549d5ba996c1042ccf0b262.webp?size=1024&format=webp&width=0&height=289" alt="Company Logo">
          <li><a href="/">Home</a></li>
          <li><a href="/Items">Items</a></li>
        </div>
        <div class="right">
          <% if (locals.user) { %>
            <a href="/dashboard"><i class="fa-solid fa-arrow-right-from-bracket"></i> Cart</a>
            <a href="/logout"><i class="fa-solid fa-xmark"></i> Logout</a>
            


            
            
            <% } else { %>
              <a href="/login"><i class="fa-solid fa-arrow-right-from-bracket"></i> Sign up</a>
              <% } %> 
        </div>
      </div>
                    <div class="pfp" id="top-right-pfp">
                      <% if (locals.user) { %>
                        <img class="pfp" src="<%= user.picture %>" alt="Your pfp">
                      <% } %>
                    </div>



                        <script>
                          const pfpElement = document.getElementById('top-right-pfp');
                          pfpElement.addEventListener('click', function() {
                          
                            window.location.href = '/dashboard';
                          });
                        </script>
